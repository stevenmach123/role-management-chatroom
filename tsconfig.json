{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "dom",
      "dom.iterable",
    
    ],
    "outDir": "lib",
    "rootDir": "src",
 
    
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,

    "esModuleInterop": true,
    "sourceMap": true,
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": true,
    "module":"NodeNext" ,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    
    "typeRoots": ["./node_modules/@types", "./src/types"]
  },
  //"hidden":true,
  "include": ["src"],
  "exclude":["src/Components/LayOut","src/Components/AddUser","src/Components/OtherComponents","src/data","src/App.*","src/index.*"]
}
